//program to print number of days in that month
//developed by Mihira date 25/09/25
#include<stdio.h>
void main()
{
int choice;
printf("Enter a number from 1 to 12");
scanf("%d",&choice);
switch(choice)
{
    case 1: case 3: case 5: case 7: case 8: case 10: case 12: printf("31 days");
    break;
    case 2: printf("28 days");
    break;
    case 4: case 6: case 9: case 11: printf("30 days");
    break;
}
}
