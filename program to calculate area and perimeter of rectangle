//Program to calculate area and perimeter of rectangle
//Developed by Mihira Date 27/8/25
#include<stdio.h>
void main()
{
    int l,b;
    int area,perimeter;
    printf("Enter length of rectangle");
    scanf("%d", &l);
    printf("Enter breadth of rectangle");
    scanf("%d", &b);
    area = l * b;
    perimeter = 2 *(l + b);
    printf("The area of rectangle is :%d\n", area);
    printf("The perimeter of rectangle is :%d\n", perimeter);
}
