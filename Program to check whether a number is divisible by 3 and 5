// Program to check if a number is divisible by 3 and 5 or not.
//Developed by Mihira  Date: 11/09/2025
#include <stdio.h>
void main () 
{
    int a;
    printf("enter a number");
    scanf("%d",&a);
    if(a%3==0)
    {
        if(a%5==0)
        printf("divisible by 3 and 5");
        else
        printf("not divisible by 3 and 5");
    }
    else
    printf("not divisible by 3 and 5");
}
